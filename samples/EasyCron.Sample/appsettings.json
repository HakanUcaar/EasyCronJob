{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "CronJobs": {
    "ConsoleCronJob": {
      "CronExpression": "* * * * *",
      "TimeZoneInfo": "Local"
    },
    "MyJob": {
      "CronExpression": "*/2 * * * *",
      "TimeZoneInfo": "Local"
    }
  }
}
