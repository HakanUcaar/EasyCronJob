{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "CronJobs": {
    "ConsoleCronJob": {
      "CronExpression": "*/10 * * * * *",
      "TimeZoneInfo": "Local",
      "CronFormat": "IncludeSeconds"
    },
    "MyJob": {
      "CronExpression": "*/2 * * * *",
      "TimeZoneInfo": "Local",
      "CronFormat": "Standard"
    }
  }
}
